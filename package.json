{
  "name": "@idevelopthings/surrealdb-client-ts",
  "description": "Slightly modified official surrealdb client",
  "author": {
    "name": "iDevelopThings",
    "url": "https://github.com/idevelopthings"
  },
  "readme": "readme.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/idevelopthings/surrealdb-client-ts.git"
  },
  "version": "0.0.2",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "types": "./index.d.ts",
  "main": "./index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "build:publish": "./scripts/publish.sh && cd dist && npm publish",
    "preview": "vite preview",
    "test": "vitest --config=vitetest.config.js",
    "coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@types/ws": "^8.5.4",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.26.0",
    "vue-tsc": "^1.0.16"
  }
}
